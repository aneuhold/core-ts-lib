{
  "name": "@aneuhold/core-ts-lib",
  "author": "Anton G. Neuhold Jr.",
  "license": "MIT",
  "version": "2.0.4",
  "description": "A core library for all of my TypeScript projects",
  "packageManager": "yarn@4.5.1",
  "type": "module",
  "scripts": {
    "tscheck": "tsc --project tsconfig.build.json --noEmit",
    "lint": "eslint",
    "test": "jest",
    "jsr:validate": "tsx ./scripts/validateJsrPublish.ts",
    "checkAll": "yarn tscheck && yarn lint && yarn test && yarn jsr:validate",
    "jsr:publish": "tsx ./scripts/publishJsr.ts",
    "upgrade:all": "yarn up"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneuhold/core-ts-lib.git"
  },
  "bugs": {
    "url": "https://github.com/aneuhold/core-ts-lib/issues"
  },
  "homepage": "https://github.com/aneuhold/core-ts-lib#readme",
  "keywords": [
    "Scripting",
    "Node.js"
  ],
  "devDependencies": {
    "@aneuhold/eslint-config": "^1.0.35",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.13",
    "eslint": "^9.13.0",
    "jest": "^29.7.0",
    "jsr": "*",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
