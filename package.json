{
  "name": "@aneuhold/core-ts-lib",
  "version": "2.0.0",
  "description": "A core library for all of my TypeScript projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "bump": "tsx ./scripts/bumpVersion.ts",
    "build": "rimraf ./lib && tsc --project tsconfig.build.json",
    "lint": "eslint",
    "test": "jest",
    "jsr:validate": "jsr publish --dry-run",
    "checkAll": "yarn build && yarn lint && yarn test && yarn jsr:validate",
    "jsr:add": "yarn dlx jsr add",
    "link:local": "yarn link",
    "unlink:local": "yarn unlink",
    "upgrade:all": "yarn up"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneuhold/core-ts-lib.git"
  },
  "keywords": [
    "Scripting",
    "Node.js"
  ],
  "author": "Anton G. Neuhold Jr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aneuhold/core-ts-lib/issues"
  },
  "homepage": "https://github.com/aneuhold/core-ts-lib#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@aneuhold/eslint-config": "^1.0.35",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.10",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "jsr": "*",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
