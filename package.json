{
  "name": "@aneuhold/core-ts-lib",
  "version": "1.1.24",
  "description": "A core library for all of my TypeScript projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "packageManager": "yarn@4.2.2",
  "scripts": {
    "pushpub": "yarn build && npm version patch && git push",
    "build": "tsc",
    "watch": "tsc -w",
    "link:local": "cd lib && yarn link",
    "unlink:local": "cd lib && yarn unlink",
    "upgrade:all": "yarn upgrade --latest",
    "test": "jest",
    "lint": "eslint"
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneuhold/core-ts-lib.git"
  },
  "keywords": [
    "Scripting",
    "Node.js"
  ],
  "author": "Anton G. Neuhold Jr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aneuhold/core-ts-lib/issues"
  },
  "homepage": "https://github.com/aneuhold/core-ts-lib#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@aneuhold/eslint-config": "^1.0.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "eslint": "^9.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
